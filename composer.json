{
    "name": "ftp-filemanager/ftp-filemanager",
    "description": "A web-based FTP client application to manage your FTP files, built with a simple MVC architecture, no frameworks or libraries are used (except my owns)",
    "type": "project",
    "license": "MIT",
    "keywords": ["ftp-filemanage", "no-framework", "mvc-pattern"],
    "authors": [
        {
            "name": "El Amrani Shakir",
            "email": "elamrani.sv.laza@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-ftp": "*",
        "lazzard/php-ftp-client": "^1.3",
        "filp/whoops": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "FTPApp\\": ["src/", "lib/"]
        }
    }
}
